# MySQL Configuration
development:
  adapter: mysql2
  database: <username>_autolab_development
  pool: 5
  username: <username>
  password: '<password>'
  socket: /var/run/mysqld/mysqld.sock
  host: localhost
  variables:
    sql_mode: NO_ENGINE_SUBSTITUTION

